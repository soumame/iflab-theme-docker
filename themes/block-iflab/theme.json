{
  "$schema": "https://schemas.wp.org/wp/6.4/theme.json",
  "settings": {
    "appearanceTools": true,
    "color": {
      "background": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultDuotone": true,
      "defaultGradients": true,
      "defaultPalette": true,
      "duotone": [],
      "gradients": [],
      "palette": [
        {
          "color": "#38bdf8",
          "name": "if-blue",
          "slug": "if-blue"
        },
        {
          "color": "#facc15",
          "name": "if-yellow",
          "slug": "if-yellow"
        },
        {
          "color": "#f3f4f6",
          "name": "if-bg",
          "slug": "if-bg"
        },
        {
          "color": "#1f2937",
          "name": "if-text",
          "slug": "if-text"
        },
        {
          "color": "#bae6fd",
          "name": "if-emphasize",
          "slug": "if-emphasize"
        },
        {
          "color": "#374151",
          "name": "if-button",
          "slug": "if-button"
        },
        {
          "color": "#f9fafb",
          "name": "if-bg2",
          "slug": "if-bg2"
        }
      ],
      "text": true
    },
    "layout": {
      "contentSize": "840px",
      "wideSize": "1100px"
    },
    "shadow": {
      "defaultPresets": true,
      "presets": []
    },
    "spacing": {
      "customSpacingSize": true,
      "spacingScale": {
        "increment": 1.5,
        "mediumStep": 1.5,
        "operator": "*",
        "steps": 7,
        "unit": "rem"
      },
      "spacingSizes": []
    },
    "typography": {
      "customFontSize": true,
      "dropCap": true,
      "fontSizes": [],
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "textDecoration": true,
      "textTransform": true
    }
  },
  "styles": {
    "blocks": {
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "var(--wp--preset--spacing--60)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontStyle": "normal",
          "fontWeight": "700"
        }
      },
      "core/paragraph": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--40)",
            "top": "var(--wp--preset--spacing--40)"
          }
        },
        "typography": {
          "letterSpacing": "0.3px",
          "lineHeight": "1.5"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--if-bg)",
      "text": "var(--wp--preset--color--if-text)"
    },
    "elements": {
      "button": {
        "color": {
          "background": "var(--wp--preset--color--if-button)",
          "text": "var(--wp--preset--color--if-bg)"
        }
      },
      "caption": {
        "color": {
          "text": "var(--wp--preset--color--if-button)"
        }
      }
    },
    "spacing": {
      "blockGap": "24px",
      "padding": {
        "bottom": "16px",
        "left": "16px",
        "right": "16px",
        "top": "16px"
      }
    }
  },
  "version": 2
}
